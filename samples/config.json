{
  "app": {
    "name": "code-phoropter",
    "env": "development",
    "port": 8080,
    "baseUrl": "http://localhost:8080"
  },
  "logging": {
    "level": "info",
    "format": "json",
    "destinations": ["stdout", "logs/app.log"]
  },
  "database": {
    "driver": "postgres",
    "host": "127.0.0.1",
    "port": 5432,
    "user": "app",
    "password": "secret",
    "name": "phoropter",
    "pool": { "min": 1, "max": 10 }
  },
  "features": {
    "ligatures": true,
    "ghostPreview": true,
    "telemetry": false
  },
  "__proto__": { "polluted": true },
  "themes": [
    { "name": "Catppuccin Mocha", "dark": true },
    { "name": "Solarized Light", "dark": false }
  ]
}
